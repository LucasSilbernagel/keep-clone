{
  "name": "keep-clone",
  "version": "1.0.0",
  "description": "A clone of Google Keep built with MongoDB, Express, React, and Node.",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node-dev server.ts",
    "start-watch": "nodemon server.ts",
    "dev": "concurrently \"npm run start-watch\" \"cd client && npm start\"",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "author": "Lucas Silbernagel",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "body-parser": "^1.19.1",
    "dotenv": "^6.1.0",
    "express": "^4.17.2",
    "mongoose": "^5.13.14",
    "recoil": "^0.7.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4"
  }
}
