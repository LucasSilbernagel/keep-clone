{
  "name": "keep-clone",
  "version": "1.0.0",
  "description": "A clone of Google Keep built with MongoDB, Express, React, and Node.",
  "main": "server.ts",
  "type": "commonjs",
  "scripts": {
    "start": "ts-node-dev server.ts",
    "start-watch": "nodemon server.ts --ignore client",
    "dev": "concurrently \"npm run start-watch\" \"cd client && npm start\"",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext \".js,.jsx,.ts,.tsx\" --ignore-path .gitignore ."
  },
  "author": "Lucas Silbernagel",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "body-parser": "^1.19.1",
    "dotenv": "^6.1.0",
    "express": "^4.17.2",
    "mongoose": "^5.13.14",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-eslint": "^10.1.0",
    "concurrently": "^4.0.1",
    "eslint": "^8.19.0",
    "eslint-config-lucas-silbernagel": "^1.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^1.18.4",
    "prettier": "^2.7.1",
    "typescript": "4.0.5"
  },
  "engines": {
    "node": "16.x"
  }
}
